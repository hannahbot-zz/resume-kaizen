<h1>My resumes</h1>

<div class="row">
  <div class="span8">
    <% resumes.each do |resume| %>
        <div class="media">
          <div class="media-body">
            <h4 class="media-heading">
              <%= link_to resume.name, resume %>
            </h4>
          </div>
        </div>
    <% end %>
  </div>
</div>
