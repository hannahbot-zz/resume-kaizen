<h1>My resumes</h1>

<div class="row">
  <div class="span8">
    <% @resumes.keys.each do |key| %>
      <% @resumes[key].each do |resume| %>
        <div class="media">
          <div class="media-body">
            <h4 class="media-heading">
              <%= link_to resume.name %>
            </h4>
          </div>
        </div>
        <% end %>
    <% end %>
  </div>
</div>
