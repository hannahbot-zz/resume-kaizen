<h1>Listing reviewer_resumes (Reviewed Resumes)</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @reviewer_resumes.each do |reviewer_resume| %>
      <tr>
        <td><%= link_to 'Show', reviewer_resume %></td>
        <td><%= link_to 'Download Resume', reviewer_resume.file_url, class: "btn btn-info" %></td>
        <td><%= link_to 'Destroy', reviewer_resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New reviewer resume', new_reviewer_resume_path %>
